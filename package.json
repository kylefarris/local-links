  "bugs": {
    "url": "https://github.com/lukekarrys/local-links/issues"
  },
  "devDependencies": {
    "browserify": "^5.10.0",
    "jshint": "^2.5.3",
    "precommit-hook": "^1.0.7",
    "run-browser": "^1.3.1",
    "tap-spec": "^0.2.0",
    "tape": "^2.14.0",
    "tape-run": "^0.3.0"
  },
  "homepage": "https://github.com/lukekarrys/local-links",
  "keywords": [
    "local",
    "links",
    "IE"
  ],
  "license": "MIT",
  "main": "local-links.js",
  "repository": {
    "type": "git",
